{
  "name": "wf-jspdf",
  "version": "1.0.0",
  "description": "A library for generating PDF documents using jsPDF",
  "exports": {
    "./pdfHeader": "./dist/pdfHeader.umd.js",
    "./pdfTable": "./dist/pdfTable.umd.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:pdfHeader": "rollup -c rollup.config.pdfHeader.mjs",
    "build:pdfTable": "rollup -c rollup.config.pdfTable.mjs",
    "build": "npm run build:pdfHeader && npm run build:pdfTable"
  },
  "dependencies": {
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.15"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "rimraf": "^5.0.10",
    "rollup": "^2.62.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pchambless/wf-jsPDF.git"
  },
  "keywords": [
    "pdf",
    "jspdf",
    "autotable"
  ],
  "author": "Paul Chambless",
  "license": "MIT"
}
